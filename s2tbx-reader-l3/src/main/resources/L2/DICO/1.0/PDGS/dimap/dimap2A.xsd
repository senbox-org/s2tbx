	<xs:complexType name="A_GENERAL_INFO_L2A">
		<xs:annotation>
			<xs:documentation>General information on L2A Tile</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TILE_ID_2A">
				<xs:annotation>
