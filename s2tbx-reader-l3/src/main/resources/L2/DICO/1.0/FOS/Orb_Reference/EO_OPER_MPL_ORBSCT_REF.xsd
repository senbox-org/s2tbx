<xsd:schema xmlns="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/FOS/Orb_Reference/" xmlns:Orb_Reference="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/FOS/Orb_Reference/" xmlns:misc="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/SY/misc/" xmlns:header="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/PDGS/header/" xmlns:orbital="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/SY/orbital/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/FOS/Orb_Reference/" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/SY/misc/" schemaLocation="../../SY/misc/misc.xsd"/>
	<xsd:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/SY/orbital/" schemaLocation="../../SY/orbital/orbital.xsd"/>
	<xsd:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/SY/orbital/" schemaLocation="../../SY/orbital/HeaderTypes_Orbit.xsd"/>
	<xsd:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/1.0/PDGS/header/" schemaLocation="../../PDGS/header/EOF_Header.xsd"/>
	<xsd:annotation>
		<xsd:documentation>Orbit Scenario Data Types</xsd:documentation>
	</xsd:annotation>
	<xsd:annotation>
