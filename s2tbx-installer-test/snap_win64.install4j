            <preActivation>File installationDir = context.getInstallationDirectory();

boolean isBeamInstallation = new File(installationDir, "VERSION.txt").exists();
isBeamInstallation &amp;= new File(installationDir, "CHANGELOG.txt").exists();
isBeamInstallation &amp;= new File(installationDir, "readme.html").exists();
isBeamInstallation &amp;= new File(installationDir, "modules").exists();

if (isBeamInstallation &amp;&amp; !context.getBooleanVariable("sys.confirmedUpdateInstallation")) {
    int selectedOption = Util.showOptionDialog("There already is a BEAM installation in " + installationDir.toString() + ".",
