	<!--List of PDI_ID-->
	<xs:simpleType name="PDI_ID">
		<xs:annotation>
			<xs:documentation>Product Data Item identification list: granules, datastrip,...)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="S2(A|B)_OPER_[\w{Lu}_]{10}_[\w{Lu}_]{4}_\d{8}T\d{6}_\d{8}T\d{6}_[\w{Lu}_]{2}"/>
			<xs:pattern value="S2(A|B)_OPER_[\w{Lu}_]{10}_[\w{Lu}_]{4}_\d{8}T\d{6}_\d{8}T\d{6}_\d{2}_[\w{Lu}_]{4}"/>
			<xs:pattern value="S2(A|B)_OPER_[\w{Lu}_]{10}_[\w{Lu}_]{4}_\d{8}T\d{6}_\d{6}_[\w{Lu}_]{5}_[\w{Lu}_]{4}"/>
			<xs:pattern value="S2(A|B)_OPER_[\w{Lu}_]{10}_[\w{Lu}_]{4}_\d{8}T\d{6}_\d{8}T\d{6}_[\w{Lu}_]{4}"/>
			<xs:pattern value="S2(A|B)_OPER_[\w{Lu}_]{10}_[\w{Lu}_]{4}_\d{8}T\d{6}_V\d{8}T\d{6}_\d{8}T\d{6}_[\w{Lu}_]{3}"/>
			<xs:pattern value="S2(A|B)_OPER_[\w{Lu}_]{10}_[\w{Lu}_]{4}_\d{8}T\d{6}_S\d{8}T\d{6}"/>
			<xs:pattern value="S2(A|B)_OPER_[\w{Lu}_]{10}_[\w{Lu}_]{4}_\d{8}T\d{6}_\d{8}T\d{6}_\d{8}T\d{6}"/>
			<xs:pattern value="S2(A|B)_OPER_[\w{Lu}_]{10}_\d{8}T\d{6}_\d{8}T\d{6}_\d{4}"/>
			<!-- S2PDGS EOF Naming Convention -->
			<!-- FOS Naming convention -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Product_ID">
		<xs:annotation>
			<xs:documentation>Product Identifier in the archive (auxiliary, DEM, GIPP,...)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Item_ID" final="union">
		<xs:annotation>
			<xs:documentation>a PDI_ID or a Product ID</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="item:PDI_ID item:Product_ID"/>
	</xs:simpleType>
	<xs:simpleType name="File_Type">
		<xs:annotation>
			<xs:documentation>The file type used by the Archive to discriminate Items (PDIs, products, auxiliary)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Latency">
