	<!--Granule ID (L0/L1A/L1B)-->
	<xs:simpleType name="GRANULE_ID">
		<xs:annotation>
			<xs:documentation>Product Data Item identification</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="S2(A|B)_OPER_[\w{Lu}_]{10}_[\w{Lu}_]{4}_\d{8}T\d{6}_\d{8}T\d{6}_\d{2}_N\d{2}.\d{2}"/>
		</xs:restriction>
	</xs:simpleType>
