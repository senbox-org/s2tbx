	<xs:complexType name="A_RADIOMETRIC_DATA_DSL1C_DSL2A">
		<xs:annotation>
			<xs:documentation>Radiometric corrections applied to L1C and L2A</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SWIR_REARRANGEMENT_PROC" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>SWIR pixels re-arrangement. A 'true' value indicates that data extraction and SWIR pixels re-arrangement have been processed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SATURATED_PIXELS_PROC" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Management of saturated pixels. A 'true' value indicates that saturated pixels has been processed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Spectral_Information_List">
				<xs:annotation>
					<xs:documentation>Spectral information which provided by the GIPP  ABSOLUTE_CALIBRATION</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Spectral_Information" maxOccurs="13">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="RESOLUTION" type="image:A_BAND_RESOLUTION"/>
									<xs:element name="Wavelength">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="MIN" type="misc:A_DOUBLE_WITH_NM_ATTR"/>
												<xs:element name="MAX" type="misc:A_DOUBLE_WITH_NM_ATTR"/>
												<xs:element name="CENTRAL" type="misc:A_DOUBLE_WITH_NM_ATTR"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Spectral_Response">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="STEP" type="misc:A_DOUBLE_WITH_NM_ATTR"/>
												<xs:element name="VALUES" type="misc:A_LIST_OF_DOUBLE">
													<xs:annotation>
														<xs:documentation>List of measures</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="physicalBand" type="image:A_PHYSICAL_BAND_NAME" use="required"/>
								<xs:attribute name="bandId" type="image:A_BAND_NUMBER" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Image_Display_Order" type="dimap:A_BAND_DISPLAY_ORDER">
				<xs:annotation>
					<xs:documentation>Relation between product image channels and on board spectral bands</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="QUANTIFICATION_VALUES_LIST">
				<xs:annotation>
					<xs:documentation>A list of L1C, L2A quantification values for digital counts on pixel level</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BOA_QUANTIFICATION_VALUE" type="misc:A_DOUBLE_WITH_UNIT_ATTR">
							<xs:annotation>
								<xs:documentation>L2A BOA Reflectance quantification value (in order to convert digit count into Bottom-Of-Atmosphere reflectance)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AOT_QUANTIFICATION_VALUE" type="misc:A_DOUBLE_WITH_UNIT_ATTR">
							<xs:annotation>
								<xs:documentation>Aeorosol Optical Thickness Map quantification value (in order to convert digit count into Aeorosol Opthical Thickness physical units)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="WVP_QUANTIFICATION_VALUE" type="misc:A_DOUBLE_WITH_UNIT_ATTR">
							<xs:annotation>
								<xs:documentation>Water Vapor Map quantification value (in order to convert digit count into water vapor physical units)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BOA_ADD_OFFSET_VALUES_LIST" type="dimap:A_RADIOMETRIC_OFFSET_LIST_2A">
                                <xs:annotation>
                                        <xs:documentation>Radiometric offsets values per spectral band</xs:documentation>
                                </xs:annotation>
                        </xs:element>	
			<xs:element name="Reflectance_Conversion">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="U" type="xs:double">
							<xs:annotation>
								<xs:documentation>Correction to take into account the Sun-Earth distance variation (this correction is computed using the acquisition date)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Solar_Irradiance_List">
							<xs:annotation>
								<xs:documentation>Reflectance parameters for each band</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="SOLAR_IRRADIANCE" maxOccurs="13">
										<xs:annotation>
											<xs:documentation>Equivalent solar irradiance</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="xs:double">
													<xs:attribute name="bandId" type="image:A_BAND_NUMBER" use="required"/>
													<xs:attribute name="unit" type="xs:string" use="required" fixed="W/m²/µm"/>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
