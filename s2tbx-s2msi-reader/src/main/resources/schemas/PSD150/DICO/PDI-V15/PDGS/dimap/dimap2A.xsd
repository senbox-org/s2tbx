<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:gs2="https://psd-15.sentinel2.eo.esa.int/doc/1.0/"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:image="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/image/"
	xmlns:date_time="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/date_time/"
	xmlns:misc="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/misc/"
	xmlns:item="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/DataAccess/item/"
	xmlns:dimap="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/PDGS/dimap/"
	xmlns:ns1="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/PDGS/base/"
	xmlns:ns2="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/PDGS/center/"
	xmlns:ns3="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/orbital/"
	xmlns:ns4="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/platform/"
	xmlns:ns5="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/geographical/"
	xmlns:ns6="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/spatio/"
	xmlns:ns7="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/tile/"
	xmlns:ns8="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/representation/"
	targetNamespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/PDGS/dimap/"
	elementFormDefault="unqualified" attributeFormDefault="unqualified" version="15">
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/image/"
		schemaLocation="../../SY/image/image.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/date_time/"
		schemaLocation="../../SY/date_time/date_time.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/PDGS/base/"
		schemaLocation="../base/base.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/PDGS/center/"
		schemaLocation="../center/center.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/orbital/"
		schemaLocation="../../SY/orbital/orbital.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/platform/"
		schemaLocation="../../SY/platform/platform.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/misc/"
		schemaLocation="../../SY/misc/misc.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/geographical/"
		schemaLocation="../../SY/geographical/geographical.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/spatio/"
		schemaLocation="../../SY/spatio/spatio.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/tile/"
		schemaLocation="../../SY/tile/tile.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/SY/representation/"
		schemaLocation="../../SY/representation/representation.xsd"/>
	<xs:import namespace="https://psd-15.sentinel2.eo.esa.int/DICO/PDI-V15/DataAccess/item/"
		schemaLocation="../../DataAccess/item/item2A.xsd"/>
	<xs:include schemaLocation="../dimap/dimap.xsd"/>
	<xs:complexType name="A_GIPP_LIST_2A">
		<xs:sequence>
			<xs:element name="GIPP_FILENAME" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>GIPP file name</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="item:GIPP_ID_2A">
							<xs:attribute name="version" type="xs:string"/>
							<xs:attribute name="type" use="required">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="GIP_ATMIMA"/>
										<xs:enumeration value="GIP_ATMSAD"/>
										<xs:enumeration value="GIP_DATATI"/>
										<xs:enumeration value="GIP_LREXTR"/>
										<xs:enumeration value="GIP_INVLOC"/>
										<xs:enumeration value="GIP_VIEDIR"/>
										<xs:enumeration value="GIP_SPAMOD"/>
										<xs:enumeration value="GIP_BLINDP"/>
										<xs:enumeration value="GIP_CLOINV"/>
										<xs:enumeration value="GIP_PRDLOC"/>
										<xs:enumeration value="GIP_R2PARA"/>
										<xs:enumeration value="GIP_R2SWIR"/>
										<xs:enumeration value="GIP_R2EOB2"/>
										<xs:enumeration value="GIP_R2EQOG"/>
										<xs:enumeration value="GIP_R2DEPI"/>
										<xs:enumeration value="GIP_R2DEFI"/>
										<xs:enumeration value="GIP_R2WAFI"/>
										<xs:enumeration value="GIP_R2L2NC"/>
										<xs:enumeration value="GIP_R2DENT"/>
										<xs:enumeration value="GIP_R2DECT"/>
										<xs:enumeration value="GIP_R2NOMO"/>
										<xs:enumeration value="GIP_R2ABCA"/>
										<xs:enumeration value="GIP_R2BINN"/>
										<xs:enumeration value="GIP_R2CRCO"/>
										<xs:enumeration value="GIP_G2PARA"/>
										<xs:enumeration value="GIP_G2PARE"/>
										<xs:enumeration value="GIP_EARMOD"/>
										<xs:enumeration value="GIP_GEOPAR"/>
										<xs:enumeration value="GIP_INTDET"/>
										<xs:enumeration value="GIP_TILPAR"/>
										<xs:enumeration value="GIP_RESPAR"/>
										<xs:enumeration value="GIP_MASPAR"/>
										<xs:enumeration value="GIP_JP2KPA"/>
										<xs:enumeration value="GIP_ECMWFP"/>
										<xs:enumeration value="GIP_DECOMP"/>
										<xs:enumeration value="GIP_OLQCPA"/>
										<xs:enumeration value="GIP_PROBAS"/>
										<xs:enumeration value="GIP_CONVER"/>
										<xs:enumeration value="GIP_L2AUSR"/>
										<xs:enumeration value="GIP_L2ACSC"/>
										<xs:enumeration value="GIP_L2ACAC"/>
										<xs:enumeration value="GIP_PROBA2"/>
										<xs:enumeration value="GIP_CLOPAR"/>
										<xs:enumeration value="GIP_HRTPAR"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_MASK_LIST_2A">
		<xs:annotation/>
		<xs:sequence>
			<xs:element name="MASK_FILENAME" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>File name of the masks. L1C, L2A: mask file types for each band and tile and mask file types band independent.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="type" use="required">
								<xs:annotation>
									<xs:documentation>Mask type</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="MSK_TECQUA"/>
										<xs:enumeration value="MSK_DEFECT"/>
										<xs:enumeration value="MSK_SATURA"/>
										<xs:enumeration value="MSK_NODATA"/>
										<xs:enumeration value="MSK_DETFOO"/>
										<xs:enumeration value="MSK_CLOLOW"/>
										<xs:enumeration value="MSK_CLOUDS"/>
										<xs:enumeration value="MSK_LANWAT"/>
										<xs:enumeration value="MSK_CLDPRB"/>
										<xs:enumeration value="MSK_SNWPRB"/>
										<xs:enumeration value="MSK_DDVPXL"/>
										<xs:enumeration value="MSK_CLASSI"/>
										<xs:enumeration value="MSK_QUALIT"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="bandId" type="image:A_BAND_NUMBER">
								<xs:annotation>
									<xs:documentation>Identifier of the band</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="detectorId" type="image:A_DETECTOR_NUMBER">
								<xs:annotation>
									<xs:documentation>Identifier of the detector</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="geometry" type="image:AN_IMAGE_GEOMETRY" use="required">
			<xs:annotation>
				<xs:documentation>Mask is defined into quicklook geometry in the inventory process and is transformed into level1 full resolution geometry in production process.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="A_PRODUCT_ORGANIZATION_2A">
		<xs:annotation>
			<xs:documentation>General PDGS Product Information on Level 2A</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Granule" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="IMAGE_ID_2A" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="item:IMAGE_ID_2A"/>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="IMAGE_FILE" type="item:IMAGE_FILE_2A" minOccurs="0"
							maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="datastripIdentifier" type="item:DATASTRIP_ID_2A"/>
					<xs:attribute name="granuleIdentifier" type="item:GRANULE_TILE_ID_2A"/>
					<xs:attribute name="imageFormat">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="JPEG2000"/>
								<xs:enumeration value="BINARY"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_L2A_Product_Info">
		<xs:annotation>
			<xs:documentation>General PDGS Product Information on Level 2A</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PRODUCT_START_TIME">
				<xs:annotation>
					<xs:documentation>Actual User Product start time defined as the Sensing Time of the first line of the first scene in the product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PRODUCT_STOP_TIME">
				<xs:annotation>
					<xs:documentation>Actual User Product stop time defined as the Sensing Time of the first line of the last scene in the product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PRODUCT_URI" type="xs:anyURI">
				<xs:annotation>
					<xs:documentation>User-Product URI. This is the User-Product URI resolved and provided by ngEO</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PROCESSING_LEVEL">
				<xs:annotation>
					<xs:documentation>Processing level of the product</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="image:A_PROCESSING_LEVEL"/>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PRODUCT_TYPE">
				<xs:annotation>
					<xs:documentation>Product type Identifier</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="S2MSI0"/>
						<xs:enumeration value="S2MSI1A"/>
						<xs:enumeration value="S2MSI1B"/>
						<xs:enumeration value="S2MSI1C"/>
						<xs:enumeration value="S2MSI2A"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PROCESSING_BASELINE" type="dimap:A_PROCESSING_BASELINE_IDENTIFICATION"/>
			<xs:element name="GENERATION_TIME" type="date_time:AN_UTC_DATE_TIME">
				<xs:annotation>
					<xs:documentation>DAG Generation Time</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PREVIEW_IMAGE_URL">
				<xs:annotation>
					<xs:documentation>Link to the preview image URL</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:anyURI"/>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PREVIEW_GEO_INFO" type="xs:string">
				<xs:annotation>
					<xs:documentation>Preview georeferencing information. Coordinate system / datum used for specifying the georeferencing information supplied with the Browse Image.
Examples: EPSG:4326, RAW</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Datatake" type="dimap:A_DATATAKE_IDENTIFICATION">
				<xs:annotation>
					<xs:documentation>General information about the MSI datatake</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Query_Options" type="dimap:A_PRODUCT_OPTIONS"/>
			<xs:element name="Product_Organisation">
				<xs:annotation>
					<xs:documentation>Logical map of the elements compounding the Level 2A User Product</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Granule_List" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="dimap:A_PRODUCT_ORGANIZATION_2A"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_PRODUCT_INFO_USERL2A">
		<xs:annotation>
			<xs:documentation>General PDGS Product Information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Product_Info" type="dimap:A_L2A_Product_Info">
				<xs:annotation>
					<xs:documentation>Common general Product Information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Product_Image_Characteristics">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Special_Values" type="dimap:A_SPECIAL_VALUES"
							minOccurs="2" maxOccurs="2">
							<xs:annotation>
								<xs:documentation>Special values encoding (NO_DATA, SATURATION) for BOA reflectance image data</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Image_Display_Order" type="dimap:A_BAND_DISPLAY_ORDER">
							<xs:annotation>
								<xs:documentation>Relation between product image channels and on board spectral bands</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="QUANTIFICATION_VALUES_LIST"
							type="dimap:A_L2A_QUANTIFICATION_VALUES_LIST">
							<xs:annotation>
								<xs:documentation>A list of L1C, L2A quantification values for digital counts on pixel level</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Reflectance_Conversion">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="U" type="xs:double">
										<xs:annotation>
											<xs:documentation>Correction to take into account the Sun-Earth distance variation (this correction is computed using the acquisition date)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Solar_Irradiance_List">
										<xs:annotation>
											<xs:documentation>Reflectance parameters for each band</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="SOLAR_IRRADIANCE" maxOccurs="13">
												<xs:annotation>
												<xs:documentation>Equivalent solar irradiance</xs:documentation>
												</xs:annotation>
												<xs:complexType>
												<xs:simpleContent>
												<xs:extension base="xs:double">
												<xs:attribute name="bandId"
												type="image:A_BAND_NUMBER" use="required"/>
												<xs:attribute name="unit" type="xs:string"
												use="required" fixed="W/m²/µm"/>
												</xs:extension>
												</xs:simpleContent>
												</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Spectral_Information_List" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Spectral information which provided by the GIPP  ABSOLUTE_CALIBRATION</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Spectral_Information" maxOccurs="13">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="RESOLUTION"
												type="image:A_BAND_RESOLUTION"/>
												<xs:element name="Wavelength">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="MIN"
												type="misc:A_DOUBLE_WITH_NM_ATTR"/>
												<xs:element name="MAX"
												type="misc:A_DOUBLE_WITH_NM_ATTR"/>
												<xs:element name="CENTRAL"
												type="misc:A_DOUBLE_WITH_NM_ATTR"/>
												</xs:sequence>
												</xs:complexType>
												</xs:element>
												<xs:element name="Spectral_Response">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="STEP"
												type="misc:A_DOUBLE_WITH_NM_ATTR"/>
												<xs:element name="VALUES"
												type="misc:A_LIST_OF_DOUBLE">
												<xs:annotation>
												<xs:documentation>List of measures</xs:documentation>
												</xs:annotation>
												</xs:element>
												</xs:sequence>
												</xs:complexType>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="physicalBand"
												type="image:A_PHYSICAL_BAND_NAME" use="required"/>
											<xs:attribute name="bandId" type="image:A_BAND_NUMBER"
												use="required"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="PHYSICAL_GAINS" maxOccurs="13">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:double">
										<xs:attribute name="bandId" use="required">
											<xs:simpleType>
												<xs:restriction base="xs:int">
												<xs:enumeration value="0"/>
												<xs:enumeration value="1"/>
												<xs:enumeration value="2"/>
												<xs:enumeration value="3"/>
												<xs:enumeration value="4"/>
												<xs:enumeration value="5"/>
												<xs:enumeration value="6"/>
												<xs:enumeration value="7"/>
												<xs:enumeration value="8"/>
												<xs:enumeration value="9"/>
												<xs:enumeration value="10"/>
												<xs:enumeration value="11"/>
												<xs:enumeration value="12"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="REFERENCE_BAND" type="xs:int"/>
						<xs:element name="Scene_Classification_List"
							type="dimap:A_L2A_SCENE_CLASSIFICATION_LIST"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_L2A_SCENE_CLASSIFICATION_LIST">
		<xs:annotation>
			<xs:documentation>A list of L2A Scene Classification IDs</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Scene_Classification_ID" type="dimap:A_L2A_SCENE_CLASSIFICATION_ID"
				minOccurs="12" maxOccurs="12">
				<xs:annotation>
					<xs:documentation>L2A Scene Classification IDs encoding (SC_NODATA, SC_NODATA, SC_SATURATED_DEFECTIVE, SC_CAST_SHADOW, SC_CLOUD_SHADOW, SC_VEGETATION, SC_NOT_VEGETATED, SC_WATER, SC_UNCLASSIFIED, SC_CLOUD_MEDIUM_PROBA, SC_CLOUD_HIGH_PROBA, SC_THIN_CIRRUS, SC_SNOW_ICE)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_L2A_SCENE_CLASSIFICATION_ID">
		<xs:annotation>
			<xs:documentation>Pixel values assigned to L2A Scene Classification Image Data </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SCENE_CLASSIFICATION_TEXT">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="SC_NODATA"/>
						<xs:enumeration value="SC_SATURATED_DEFECTIVE"/>
						<xs:enumeration value="SC_CAST_SHADOW"/>
						<xs:enumeration value="SC_CLOUD_SHADOW"/>
						<xs:enumeration value="SC_VEGETATION"/>
						<xs:enumeration value="SC_NOT_VEGETATED"/>
						<xs:enumeration value="SC_WATER"/>
						<xs:enumeration value="SC_UNCLASSIFIED"/>
						<xs:enumeration value="SC_CLOUD_MEDIUM_PROBA"/>
						<xs:enumeration value="SC_CLOUD_HIGH_PROBA"/>
						<xs:enumeration value="SC_THIN_CIRRUS"/>
						<xs:enumeration value="SC_SNOW_ICE"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SCENE_CLASSIFICATION_INDEX" type="xs:int"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_QUALITY_INDICATORS_INFO_USER_PROD_L1C_L2A">
		<xs:annotation>
			<xs:documentation>Quality Indicators information on product level (L2A + L1C Technical assessment info)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="dimap:A_QUALITY_INDICATORS_INFO_USER_PROD_L1B_L1C">
				<xs:sequence>
					<xs:element name="Image_Content_QI" type="dimap:A_L2A_IMG_CONTENT_QI"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AN_IMAGE_DATA_INFO_DSL1C_DSL2A">
		<xs:annotation>
			<xs:documentation>List of L2A tiles + L1C Geometric and Radiometric info</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Tiles_Information">
				<xs:annotation>
