							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="REFERENCE_BAND" type="image:A_BAND_NUMBER">
										<xs:annotation>
											<xs:documentation>reference band used for geometric refining/registration </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Reference_Image_List">
										<xs:annotation>
											<xs:documentation>Reference images used for geometric model refining processing</xs:documentation>
