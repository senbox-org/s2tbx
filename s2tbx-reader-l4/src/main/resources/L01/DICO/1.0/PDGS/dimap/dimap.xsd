											<xs:selector xpath="./Footprint"/>
											<xs:field xpath="@detector_id"/>
										</xs:unique>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
							<xs:unique name="band_id_unitary_footprint_inventory_unique">
								<xs:selector xpath="./Unitary_Footprint"/>
								<xs:field xpath="@band_id"/>
							</xs:unique>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="RASTER_CS_TYPE">
				<xs:annotation>
					<xs:documentation>Pixel representation. Values is "POINT" for N0 and N1 levels. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="POINT"/>
					</xs:restriction>
				</xs:simpleType>
