							<xs:restriction base="xs:string">
								<xs:enumeration value="REFINED"/>
								<xs:enumeration value="NOT_REFINED"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
