											<xs:attribute name="gspId" use="required">
												<xs:simpleType>
													<xs:restriction base="image:A_GSP_ID"/>
												</xs:simpleType>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
							<xs:unique name="gsp_id_unique">
								<xs:selector xpath="./GSP"/>
								<xs:field xpath="@gsp_id"/>
							</xs:unique>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="usedModel" use="required">
