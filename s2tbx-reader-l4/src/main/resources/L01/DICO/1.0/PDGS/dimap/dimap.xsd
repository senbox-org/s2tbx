				<gs2:DOC>
					<gs2:DEFINITION/>
					<gs2:CREATED_BY>RADIO_S2</gs2:CREATED_BY>
					<gs2:EXPERTISE>L1B_PRODUCT</gs2:EXPERTISE>
					<gs2:EXPERTISE>L1C_PRODUCT</gs2:EXPERTISE>
				</gs2:DOC>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Equalized_Band_List" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of equalized bands (if any)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Equalized_Band" maxOccurs="13">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="OFFSET_PROC" type="xs:boolean" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Offset correction: A 'true' value indicates that offset correction has been done</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="DARK_SIGNAL_NON_UNIFORMITY_PROC" type="xs:boolean" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Dark signal non uniformity correction: A 'true' value indicates that dark signal non uniformity correction has been done</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="bandId" type="image:A_BAND_NUMBER" use="required"/>
