	<xs:complexType name="AN_IMAGE_POINTING_ANGLES">
		<xs:annotation>
			<xs:documentation>Pointing angles </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PSI_X" type="misc:A_DOUBLE_WITH_DEG_UNIT_ATTR">
				<xs:annotation>
					<xs:documentation>Along lines</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PSI_Y" type="misc:A_DOUBLE_WITH_DEG_UNIT_ATTR">
				<xs:annotation>
					<xs:documentation>Along columns</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_YAW_PITCH_ROLL_ANGLES">
		<xs:annotation>
			<xs:documentation>Yaw, pitch, and roll angles</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ROLL" type="misc:A_DOUBLE_WITH_DEG_UNIT_ATTR"/>
			<xs:element name="PITCH" type="misc:A_DOUBLE_WITH_DEG_UNIT_ATTR"/>
			<xs:element name="YAW" type="misc:A_DOUBLE_WITH_DEG_UNIT_ATTR"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_ZENITH_AND_AZIMUTH_ANGLES">
		<xs:annotation>
			<xs:documentation>Zenith and azimuth angles </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ZENITH_ANGLE" type="misc:A_DOUBLE_WITH_DEG_UNIT_ATTR"/>
			<xs:element name="AZIMUTH_ANGLE" type="misc:A_DOUBLE_WITH_DEG_UNIT_ATTR">
				<xs:annotation>
					<xs:documentation>The range of the angle is into [0,360Â°[ </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_DATASET_IDENTIFICATION">
		<xs:annotation>
			<xs:documentation>Dataset identification</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DATASET_NAME" type="xs:string">
				<xs:annotation>
					<xs:documentation>String that identifies a Sentinel2 dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="COPYRIGHT" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="A_METADATA_PROFILE">
		<xs:annotation>
