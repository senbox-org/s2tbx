						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
				<xs:unique name="pos_unique">
					<xs:selector xpath="./Located_Geometric_Header"/>
					<xs:field xpath="@pos"/>
				</xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
