	<xs:complexType name="A_GEOMETRIC_HEADER_LIST_EXPERTISE">
		<xs:sequence>
			<xs:element name="Geometric_Header" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information is provided on QL geometry or on the full resolution</xs:documentation>
					<xs:appinfo>
						<gs2:DOC>
							<gs2:DEFINITION/>
							<gs2:CREATED_BY>INIT_LOC_INV</gs2:CREATED_BY>
							<gs2:UPDATED_BY>INIT_LOC_PROD</gs2:UPDATED_BY>
						</gs2:DOC>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="GPS_TIME" type="date_time:A_GPS_DATE_TIME"/>
						<xs:element name="LINE_INDEX">
							<xs:simpleType>
								<xs:restriction base="xs:int">
									<xs:minInclusive value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Pointing_Angles">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Satellite_Reference" type="dimap:A_YAW_PITCH_ROLL_ANGLES">
										<xs:annotation>
											<xs:documentation>Pointing angles in satellite reference frame </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Image_Reference" type="dimap:AN_IMAGE_POINTING_ANGLES" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Pointing angles in focal plane referential </xs:documentation>
											<xs:appinfo>
												<gs2:DOC>
													<gs2:DEFINITION>Pointing angles in focal plane referential</gs2:DEFINITION>
												</gs2:DOC>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Located_Geometric_Header" minOccurs="3" maxOccurs="3">
							<xs:annotation>
								<xs:documentation>Position of the swath. Indeed, data are computed for each line index and for col = first_col, centre_col, and last_col, defining the begin, the centre and the end of the swath.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ORIENTATION" type="misc:A_DOUBLE_WITH_DEG_UNIT_ATTR">
										<xs:annotation>
											<xs:documentation>Track orientation. Also called "CAPE". The range of the angle is into [0,360Â°[</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Incidence_Angles" type="dimap:A_ZENITH_AND_AZIMUTH_ANGLES">
										<xs:annotation>
											<xs:documentation>Incidence angles</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Solar_Angles" type="dimap:A_ZENITH_AND_AZIMUTH_ANGLES"/>
									<xs:element name="Pixel_Size">
										<xs:annotation>
											<xs:documentation>Full resolution pixel sizes along and across track in meters</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ALONG_TRACK" type="misc:A_DOUBLE_WITH_M_UNIT_ATTR"/>
												<xs:element name="ACROSS_TRACK" type="misc:A_DOUBLE_WITH_M_UNIT_ATTR"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="pos" type="dimap:A_SWATH_POSITION" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="geometry" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="FULL_RESOLUTION"/>
								<xs:enumeration value="QL"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
				<xs:unique name="pos_expertise_unique">
					<xs:selector xpath="./Located_Geometric_Header"/>
					<xs:field xpath="@pos"/>
				</xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
