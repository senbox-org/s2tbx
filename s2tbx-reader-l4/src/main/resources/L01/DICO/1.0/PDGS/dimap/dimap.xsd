								<xs:sequence>
									<xs:element name="Unitary_Footprint" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Footprint" maxOccurs="unbounded">
													<xs:complexType>
														<xs:complexContent>
															<xs:extension base="geographical:A_GML_POLYGON_2D">
																<xs:attribute name="detectorId" type="image:A_DETECTOR_NUMBER" use="required"/>
