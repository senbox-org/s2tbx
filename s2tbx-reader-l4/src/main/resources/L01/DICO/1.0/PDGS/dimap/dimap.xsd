					<xs:sequence>
						<xs:element name="Global">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="RMOY" type="misc:A_DOUBLE_WITH_S_UNIT_ATTR">
										<xs:annotation>
											<xs:documentation>Global least square residual </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="GSP_List">
