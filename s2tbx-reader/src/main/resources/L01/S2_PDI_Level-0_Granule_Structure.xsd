<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="https://psd-12.sentinel2.eo.esa.int/PSD/S2_PDI_Level-0_Granule_Structure.xsd" targetNamespace="https://psd-12.sentinel2.eo.esa.int/PSD/S2_PDI_Level-0_Granule_Structure.xsd" elementFormDefault="qualified" version="12">
	<xs:element name="Level-0_Granule" type="Level-0_Granule"/>
	<xs:complexType name="Level-0_Granule">
		<xs:annotation>
			<xs:documentation>Folder</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Inventory_Metadata">
				<xs:annotation>
					<xs:documentation>XML File</xs:documentation>
				</xs:annotation>
				<xs:complexType/>
			</xs:element>
			<xs:element name="Level-0_Granule_Metadata_File">
				<xs:annotation>
					<xs:documentation>XML Metadata File</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="General_Info"/>
						<xs:element name="Geometric_Info">
							<xs:complexType/>
						</xs:element>
						<xs:element name="Quality_Indicators_Info">
							<xs:complexType/>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="IMG_DATA">
				<xs:annotation>
					<xs:documentation>Image Folder</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ISP_Files" minOccurs="13" maxOccurs="13">
							<xs:annotation>
								<xs:documentation>Set of 13 binary files, one image file per band</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="QI_DATA">
				<xs:annotation>
					<xs:documentation>Folder</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OLQC_Report">
							<xs:annotation>
								<xs:documentation>OLQC reports XML formatted</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="manifest.safe">
				<xs:annotation>
					<xs:documentation>XML File</xs:documentation>
				</xs:annotation>
				<xs:complexType/>
			</xs:element>
			<xs:element name="rep_info">
				<xs:annotation>
					<xs:documentation>Folder</xs:documentation>
				</xs:annotation>
				<xs:complexType/>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
